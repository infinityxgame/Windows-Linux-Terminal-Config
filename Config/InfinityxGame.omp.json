{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8000ff",
          "style": "plain",
          "template": "üöÄ {{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#FFBB00",
          "powerline_symbol": "\ue0c4",
          "properties": {
            "time_format": "03:04"
          },
          "style": "plain",
          "template": "üïî{{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "#87FF00",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "üìÅ {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#5FD7FF",
          "properties": {
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "",
            "branch_icon": "",
            "branch_identical_icon": "",
            "cherry_pick_icon": "",
            "commit_icon": "",
            "fetch_status": true,
            "merge_icon": "",
            "rebase_icon": "",
            "revert_icon": "",
            "tag_icon": ""
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if .Staging.Changed }}<#87FF00> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#D75F00> \u25cf {{ .Working.String }}</>{{ end }}",
          "type": "git"
        },
        {
          "foreground": "#D75F00",
          "powerline_symbol": "\ue0c4",
          "properties": {
            "style": "austin",
            "threshold": 1
          },
          "style": "plain",
          "template": " ‚åõ{{ .FormattedMs }}",
          "type": "executiontime"
        },
        {
          "foreground": "#D75F00",
          "style": "plain",
          "template": " Œª ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
